{
  "name": "koa-extern",
  "version": "0.1.3",
  "description": "extend for koa",
  "main": "./index.js",
  "scripts": {
    "test": "jest --forceExit",
    "cov": "npm run test -- --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "debug": "nodemon ./index.js",
    "lint": "standard **/*.js *.js --fix"
  },
  "keywords": [
    "koa2",
    "extend",
    "xtx1130"
  ],
  "author": "xtx1130",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.2.0",
    "koa-body": "^2.3.0",
    "koa-router": "^7.2.0",
    "koa2-request-middleware": "^2.1.3",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "tough-cookie": "^2.3.2"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "istanbul": "^0.4.5",
    "jest": "^23.4.2",
    "nodemon": "^1.12.0",
    "regenerator-runtime": "^0.10.5",
    "standard": "^11.0.1",
    "testing": "^1.1.1"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xtx1130/koa-extern"
  },
  "jest": {
    "testMatch": [
      "**/test/test/**"
    ],
    "testURL": "http://localhost/",
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "!**/test/**",
      "**/*.js",
      "!**/coverage/**"
    ]
  },
  "standard": {
    "ignore": [
      "./converage/*",
      "LICENSE",
      "note",
      "readme.md",
      "package.json",
      "package-lock.json",
      "*.yml",
      "./node_modules/*"
    ]
  }
}
